<?xml version="1.0"?>
<VirtualBox xmlns="http://www.virtualbox.org/"
  version="1.12-linux">
  <Global>
    <ExtraData>
      <ExtraDataItem name="GUI/Input/AutoCapture"
        value="false" />
      <ExtraDataItem name="GUI/UpdateDate"
        value="never" />
    </ExtraData>
    <SystemProperties defaultMachineFolder="{{ user_storage }}/vms"
      defaultHardDiskFormat="VDI"
      VRDEAuthLibrary="VBoxAuth"
      webServiceAuthLibrary="VBoxAuth"
      LogHistoryCount="3"
      exclusiveHwVirt="true" />
  </Global>
</VirtualBox>
